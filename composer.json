{
  "name": "gtsciences/gt_drupal_kickoff",
  "description": "GT Drupal installer for a more civilized age",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "authors": [
    {
      "name": "Michael Sheldon, ARCS",
      "role": "Ass. Dir Web Services"
    }
  ],
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "email": "web@cos.gatech.edu",
    "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "gt_drupal_profile": {
      "type": "vcs",
      "url": "https://github.com/konfuzed/gt_drupal_profile.git"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "composer/installers": "^2.0",
    "cweagans/composer-patches": "^1.7",
    "drupal/core-composer-scaffold": "^9.3",
    "drupal/core-recommended": "^9.3",
    "deployer/dist": "^7.0@rc",
    "drush/drush": "^11.0",
    "gtsciences/gt_drupal_profile": "dev-main",
    "oomphinc/composer-installers-extender": "^2.0",
    "webflo/drupal-finder": "^1.2",
    "wikimedia/composer-merge-plugin": "^2.0"
  },
  "require-dev": {
    "drupal/core-dev": "^9.3"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "deployer/deployer": true,
      "drupal/*": true,
      "drush/*": true,
      "gtsciences/*": true,
      "oomphinc/composer-installers-extender": true,
      "webflo/drupal-finder": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "autoload": {
    "files": ["load.environment.php"]
  },
  "enable-patching": true,
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "gitignore": false,
      "file-mapping": {
        "[project-root]/.gitattributes": false,
        "[project-root]/.editorconfig": {
          "mode": "replace",
          "path": "assets/editorconfig",
          "overwrite": true
        },
        "[project-root]/sites/default/settings.php": {
          "mode": "replace",
          "path": "assets/settings.php",
          "overwrite": false
        }
      }
    },
    "installer-paths": {
      "web/core": "type:drupal-core",
      "web/profiles/{$name}": "type:drupal-profile",
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": "type:drupal-module",
      "web/themes/contrib/{$name}": "type:drupal-theme",
      "drush/Commands/contrib/{$name}": "type:drupal-drush",
      "web/modules/custom/{$name}": "type:drupal-custom-module",
      "web/themes/custom/{$name}": "type:drupal-custom-theme"
    },
    "merge-plugin": {
      "require": [
        "web/modules/contrib/*/composer.json",
        "web/modules/custom/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": false
    }
  },
  "version": "0.1.0"
}
