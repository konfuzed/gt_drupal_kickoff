{
  "name": "gtsciences/gt_drupal_kickoff",
  "description": "GT Drupal installer for a more civilized age",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/konfuzed/gt_drupal_profile.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/konfuzed/gt_drupal_sso.git"
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer/installers": "^1.7",
    "cweagans/composer-patches": "^1.7",
    "drupal/address": "^1.9",
    "drupal/admin_toolbar": "^3.0",
    "drupal/blazy": "^2.5",
    "drupal/charts": "^5.0@alpha",
    "drupal/codemirror_editor": "^1.7",
    "drupal/config_entity_revisions": "^1.8",
    "drupal/config_ignore": "^3.0@beta",
    "drupal/config_import_single": "^1.0",
    "drupal/config_split": "^2.0@beta",
    "drupal/core-composer-scaffold": "^9.3",
    "drupal/core-project-message": "^9.3",
    "drupal/core-recommended": "^9.3",
    "drupal/crop": "^2.1",
    "drupal/ctools": "^3.7",
    "drupal/diff": "^1.0",
    "drupal/editoria11y": "^1.0",
    "drupal/embed": "^1.4",
    "drupal/entity_browser": "^2.5",
    "drupal/entity_embed": "^1.2",
    "drupal/encrypt": "^3.0",
    "drupal/env_variables": "^2.1",
    "drupal/environment_indicator": "^4.0",
    "drupal/field_group": "^3.2",
    "drupal/focal_point": "^1.5",
    "drupal/gin": "^3.0@alpha",
    "drupal/gin_lb": "^1.0@beta",
    "drupal/gin_login": "^1.0",
    "drupal/gin_toolbar": "^1.0@beta",
    "drupal/iframe": "^2.12",
    "drupal/image_effects": "^3.1",
    "drupal/inline_block_title_automatic": "^1.0",
    "drupal/key": "^1.15",
    "drupal/layout_builder_component_attributes": "^2.0",
    "drupal/layout_builder_modal": "^1.1",
    "drupal/layout_builder_operation_link": "^1.0",
    "drupal/layout_builder_restrictions": "^2.10",
    "drupal/layout_builder_styles": "^1.0@beta",
    "drupal/lb_direct_add": "^1.2",
    "drupal/menu_link_attributes": "^1.2",
    "drupal/markdown": "^3.0@rc",
    "drupal/masquerade": "^2.0@beta",
    "drupal/metatag": "^1.16",
    "drupal/module_filter": "^3.2",
    "drupal/oembed_providers": "^2.0",
    "drupal/pathauto": "^1.8",
    "drupal/redirect": "^1.7",
    "drupal/redirect_metrics": "^1.0@rc",
    "drupal/scheduler": "^2.0@alpha",
    "drupal/simple_sitemap": "^4.0",
    "drupal/social_media_links": "^2.8",
    "drupal/sodium": "^2.2",
    "drupal/svg_image": "^1.15",
    "drupal/telephone_validation": "^2.3",
    "drupal/token": "^1.10",
    "drupal/twig_tweak": "^3.1",
    "drupal/views_bulk_edit": "^2.6",
    "drupal/views_database_connector": "^1.4",
    "drupal/views_data_export": "^1.0",
    "drupal/webform": "^6.1",
    "drupal/honeypot": "^2.0",
    "drush/drush": "^11.0@rc",
    "gtsciences/gt_drupal_profile": "dev-main",
    "gtsciences/gt_drupal_sso": "dev-main",
    "league/commonmark": "^1.6",
    "oomphinc/composer-installers-extender": "^2.0",
    "wikimedia/composer-merge-plugin": "^2.0"
  },
  "require-dev": {
    "drupal/ckeditor5_dev": "^1.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/*": true,
      "gtsciences/*": true,
      "oomphinc/composer-installers-extender": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "enable-patching": true,
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "merge-plugin": {
        "require": [
            "web/modules/custom/gt_drupal_sso/composer.json",
            "web/modules/contrib/webform/composer.json"
        ],
        "recurse": true,
        "replace": false,
        "ignore-duplicates": false,
        "merge-dev": true,
        "merge-extra": false,
        "merge-extra-deep": false,
        "merge-scripts": false
    },
    "drupal-core-project-message": {
      "include-keys": [
        "homepage",
        "support"
      ],
      "post-create-project-cmd-message": [
        "<bg=blue;fg=white>                             </>",
        "<bg=blue;fg=white>  Congratulations, you&apos;ve installed the Drupal codebase  </>",
        "<bg=blue;fg=white>  from the drupal/legacy-project template!         </>",
        "<bg=blue;fg=white>                             </>",
        "",
        "<bg=yellow;fg=black>Next steps</>:",
        "  * Install the site: https://www.drupal.org/docs/8/install",
        "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
        "  * Get support: https://www.drupal.org/support",
        "  * Get involved with the Drupal community:",
        "    https://www.drupal.org/getting-involved",
        "  * Remove the plugin that prints this message:",
        "    composer remove drupal/core-project-message"
      ]
    }
  }
}
